# MCP Server Environment Variables
# Copy this file to .env and fill in your actual API keys

# Firecrawl API Key
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# OpenRouter API Key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Project Configuration
PROJECT_PATH=/home/vpcuser/qubinode_kvmhost_setup_collection
ADR_DIRECTORY=/home/vpcuser/qubinode_kvmhost_setup_collection/docs/adrs
LOG_LEVEL=ERROR
EXECUTION_MODE=full
AI_MODEL=anthropic/claude-3-sonnet

# MCP Data Directory
DATA_DIR=.vscoder
TEMPLATES_USE=en
ENABLE_GUI=true
